pipeline {
    agent none
    stages {
        stage('Run Tests') {
            parallel {
                stage('Test On Windows') {
                    agent {
                        label "windows"
                    }
                    steps {
                        echo hii
                    }
                    
                }
                stage('Test On Docker') {
                    agent {
                        label "linux"
                    }
                    steps {
                        echo hello
                    }
                    
                }
            }
        }
    }
}


