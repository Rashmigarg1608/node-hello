pipeline {
    agent none 
    stages {
        stage('Run Tests') {
            parallel {
                stage('Test') {
                agent {
                        label "windows"
                    }
                    
                    steps {
                        echo hii
                    }
                    post {
                        always {
                            junit "**/TEST-*.xml"
                        }
                    }
                    
                }
                stage('Test') {
                agent {
                        label "docker"
                    }
                    
                    steps {
                        echo hello
                    }
                    
                }
            }
        }
    }
}


